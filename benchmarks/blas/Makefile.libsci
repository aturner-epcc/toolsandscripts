SRC	= dgemv_benchmark.f90

OBJ	=$(SRC:.f90=.o)

FC      = ftn
PP      = 

LDFLAGS =
FFLAGS  =  -O3 $(PP)
LIBS    = 

PRG	= dgemv_libsci.x

main:	$(PRG) 

.f90.o:	Makefile.libsci
	$(FC) -c $(FFLAGS) $<

$(PRG):$(OBJ) Makefile.libsci
	$(FC) $(LDFLAGS) -o $@ $(OBJ) $(LIBS)

clean:
	rm -fr $(TMP) $(OBJ) $(PRG) core
