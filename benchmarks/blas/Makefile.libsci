SRC	= blas_benchmark.c

OBJ	=$(SRC:.c=.o)

CC      = cc
PP      = 

LDFLAGS =
CFLAGS  =  -O3 $(PP)
LIBS    = 

PRG	= blas_libsci

main:	$(PRG) 

.c.o:	Makefile.libsci
	$(CC) -c $(CFLAGS) $<

$(PRG):$(OBJ) Makefile.libsci
	$(CC) $(LDFLAGS) -o $@ $(OBJ) $(LIBS)

clean:
	rm -fr $(TMP) $(OBJ) $(PRG) core
